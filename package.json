{
  "name": "opencode-agent-automation",
  "version": "1.0.0",
  "description": "Automated OpenCode AI agent delegation system for Claude and OpenCode integration",
  "main": "index.js",
  "bin": {
    "opencode-agents": "./bin/opencode-agents.js"
  },
  "scripts": {
    "install": "node scripts/install.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:coverage:watch": "jest --coverage --watch",
    "test:coverage:report": "jest --coverage --coverageReporters=text-lcov | npx coveralls",
    "test:integration": "jest --config tests/integration/jest.config.js",
    "test:integration:watch": "jest --config tests/integration/jest.config.js --watch",
    "test:all": "npm run test && npm run test:integration",
    "coverage:open": "open coverage/lcov-report/index.html || xdg-open coverage/lcov-report/index.html",
    "setup": "bash install.sh"
  },
  "keywords": [
    "opencode",
    "claude",
    "ai",
    "automation",
    "agent",
    "delegation",
    "development",
    "assistant"
  ],
  "author": "OpenCode Agent Automation",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/mediafill/opencode-agent-automation.git"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "devDependencies": {
    "@testing-library/dom": "^9.3.4",
    "@testing-library/jest-dom": "^6.8.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-websocket-mock": "^2.5.0"
  },
  "files": [
    "bin/",
    "scripts/",
    "templates/",
    "install.sh",
    "README.md",
    "LICENSE"
  ]
}
